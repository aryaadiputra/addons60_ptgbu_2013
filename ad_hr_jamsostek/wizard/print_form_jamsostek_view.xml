<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="jamsostek_form_report_view" model="ir.ui.view">
            <field name="name">Jamsostek Form Report</field>
            <field name="model">form.jamsostek.report</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            	<form string="Print Tax Statements">
	               <field name="name"/>
	               <separator colspan="4"/>
                    <group colspan="4" col="6">
                        <button special="cancel" string="Cancel" icon="gtk-cancel"/>
                        <button name="print_form_jamsostek_report" string="Print Form Jamsostek" type="object" icon="gtk-ok"/>
                    </group>
	             </form>
            </field>
        </record>
		
        <act_window name="Print Jamsostek Form"
            res_model="form.jamsostek.report"
            src_model="hr.employee"
            view_mode="form"
            target="new"
            key2="client_action_multi"
            id="action_jamsostek_form_report"/>
        
    </data>
</openerp>
